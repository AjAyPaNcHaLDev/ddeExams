            android:src="@drawable/ic_star" />

        <ImageView
            android:id="@+id/icon_ek6"
            android:layout_width="15dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="1dp"
            android:contentDescription="ic_star"
            android:src="@drawable/ic_star" />

        <ImageView
            android:id="@+id/icon_ek7"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="1dp"
            android:contentDescription="ic_star"
            android:src="@drawable/ic_star" />

        <ImageView
            android:id="@+id/icon_ek8"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="1dp"
            android:contentDescription="ic_star"
            android:src="@drawable/ic_star" />

        <ImageView
            android:id="@+id/icon_ek9"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="80dp"
            android:layout_marginTop="1dp"
            android:contentDescription="ic_star"
            android:src="@drawable/ic_star" />

        <TextView
            android:id="@+id/_5_0__21_reviews_"
            android:layout_width="126dp"
            android:layout_height="26dp"
            android:layout_marginLeft="103dp"
            android:layout_marginTop="0dp"
            android:fontFamily="Poppins"
            android:text="@string/_5_0__21_reviews__string"
            android:textColor="@color/_5_0__21_reviews__color"
            android:textSize="12sp" />

    </RelativeLayout>


        <RelativeLayout android:id="@+id/time"
            android:layout_marginTop="104dp"
            android:layout_marginStart="144dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >


            <RelativeLayout android:id="@+id/icon_ek10"
                android:layout_marginTop="1dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <View   android:id="@+id/oval_ek1"
                    android:background="@drawable/oval_ek1_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="19dp"
                    android:layout_height="19dp" />

                <ImageView android:id="@+id/path_ek26"
                    android:src="@drawable/path_ek26"
                    android:contentDescription="path_ek26"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_width="3dp"
                    android:layout_height="6dp" />

            </RelativeLayout>

            <TextView
                android:id="@+id/_25_class"
                android:layout_width="75dp"
                android:layout_height="26dp"
                android:layout_marginLeft="23dp"
                android:layout_marginTop="0dp"
                android:fontFamily="Poppins"
                android:text="@string/_25_class_string"
                android:textColor="@color/_25_class_color"
                android:textSize="12sp" />

        </RelativeLayout>

