<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/card_ek3"
    android:layout_marginTop="43dp"
    android:layout_marginStart="5dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageView
        android:id="@+id/rectangle_ek4"
        android:layout_width="386dp"
        android:layout_height="130dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="1dp"
        android:contentDescription="rectangle_ek4"
        android:src="@drawable/rectangle_ek4" />


        <RelativeLayout android:id="@+id/image"
            android:layout_marginTop="0dp"
            android:layout_marginStart="0dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <View   android:id="@+id/mask_ek2"
                android:background="@drawable/mask_ek2_shape"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_width="130dp"
                android:layout_height="131dp" />

            <View
                android:id="@+id/mask_ek3"
                android:layout_width="130dp"
                android:layout_height="131dp"
